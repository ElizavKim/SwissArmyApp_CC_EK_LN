<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="790.0" prefWidth="1206.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.swissarmyapp.LoveCalculator">
   <children>
      <Circle fill="WHITE" layoutX="600.0" layoutY="173.0" radius="103.0" stroke="WHITE" strokeType="INSIDE" />
      <TextField fx:id="nameInput" layoutX="453.0" layoutY="386.0" prefHeight="56.0" prefWidth="301.0" text="name 1">
         <font>
            <Font size="17.0" />
         </font></TextField>
      <TextField fx:id="nameInput2" layoutX="453.0" layoutY="459.0" prefHeight="56.0" prefWidth="301.0" text="name 2">
         <font>
            <Font size="17.0" />
         </font></TextField>
      <Button fx:id="pushButton" layoutX="541.0" layoutY="555.0" mnemonicParsing="false" onAction="#initialize" prefHeight="48.0" prefWidth="125.0" text="calculate">
         <font>
            <Font size="17.0" />
         </font></Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToActivities" prefHeight="48.0" prefWidth="100.0" text="back">
         <font>
            <Font size="17.0" />
         </font></Button>
      <ImageView fitHeight="144.0" fitWidth="143.0" layoutX="528.0" layoutY="112.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../heart.png" />
         </image>
      </ImageView>
      <Label layoutX="308.0" layoutY="291.0" text="enter you &amp;  your crush's name to find your compatibility!" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="resultLabel" layoutX="453.0" layoutY="333.0" prefHeight="40.0" prefWidth="575.0">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
