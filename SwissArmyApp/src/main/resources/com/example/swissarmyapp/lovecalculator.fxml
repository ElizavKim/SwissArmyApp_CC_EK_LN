<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="790.0" prefWidth="1206.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.swissarmyapp.LoveCalculator">
   <children>
      <TextField fx:id="nameInput" layoutX="453.0" layoutY="386.0" prefHeight="56.0" prefWidth="301.0" text="name 1">
         <font>
            <Font size="17.0" />
         </font></TextField>
      <TextField fx:id="nameInput2" layoutX="453.0" layoutY="459.0" prefHeight="56.0" prefWidth="301.0" text="name 2">
         <font>
            <Font size="17.0" />
         </font></TextField>
      <Button fx:id="pushButton" layoutX="541.0" layoutY="555.0" mnemonicParsing="false" onAction="#initialize" prefHeight="48.0" prefWidth="125.0" text="calculate">
         <font>
            <Font size="17.0" />
         </font></Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToActivities" prefHeight="48.0" prefWidth="100.0" text="back">
         <font>
            <Font size="17.0" />
         </font></Button>
      <ImageView fitHeight="151.0" fitWidth="154.0" layoutX="528.0" layoutY="112.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../heart.png" />
         </image>
      </ImageView>
      <Label fx:id="resultLabel" alignment="CENTER" layoutX="293.0" layoutY="326.0" prefHeight="30.0" prefWidth="622.0" text="love calculator" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="303.0" layoutY="273.0" text="enter you + your's crush's name to find your compatibility!" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
